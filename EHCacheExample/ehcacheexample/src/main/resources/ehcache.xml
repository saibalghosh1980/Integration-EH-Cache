<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd"
	updateCheck="true" monitoring="autodetect" dynamicConfig="true">
	<!-- Get a sample configuration file from the location http://www.ehcache.org/ehcache.xml -->
	<!-- Location of diskstore -->
	<diskStore path="/home/wolverine/SKG-DEV-SPACE/JAVA/cache" />
	<cacheManagerEventListenerFactory
		class="" />
	<cache name="oup-cache-allItemsCache"
		diskExpiryThreadIntervalSeconds="30" maxElementsInMemory="2"
		maxBytesLocalDisk="1G" eternal="false" overflowToDisk="true"
		timeToLiveSeconds="300" timeToIdleSeconds="0"
		memoryStoreEvictionPolicy="FIFO" clearOnFlush="true"
		diskPersistent="false" transactionalMode="off">
	</cache>
</ehcache>